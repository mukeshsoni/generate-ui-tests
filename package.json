{
  "name": "generate-ui-tests",
  "version": "1.2.4",
  "description": "generate-ui-tests React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": ["css", "es", "lib", "umd"],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "jest",
    "test: coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "nwb test-react --coverage",
    "cypress:open": "cypress open",
    "deploy": "gh-pages -d demo/dist"
  },
  "jest": {
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "transform": {
      "^.+\\.js$": "./jest.transform.js"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "react-collapsible": "^2.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-draggable": "^3.0.5",
    "react-dropdown": "^1.4.2",
    "react-modal": "^3.4.4",
    "react-switch": "^2.3.2",
    "react-syntax-highlighter": "^7.0.2",
    "stringify-object": "^3.2.2"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cypress": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "gh-pages": "^1.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "nwb": "0.21.x",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "https://github.com/mukeshsoni/generate-ui-tests",
  "keywords": ["react-component"]
}
